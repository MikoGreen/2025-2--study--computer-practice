#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/narogozhina/work/study/2025-2026/2025-2--study--computer-practice/node_modules/.pnpm/conventional-changelog@7.1.1_conventional-commits-filter@5.0.0/node_modules/conventional-changelog/dist/cli/node_modules:/home/narogozhina/work/study/2025-2026/2025-2--study--computer-practice/node_modules/.pnpm/conventional-changelog@7.1.1_conventional-commits-filter@5.0.0/node_modules/conventional-changelog/dist/node_modules:/home/narogozhina/work/study/2025-2026/2025-2--study--computer-practice/node_modules/.pnpm/conventional-changelog@7.1.1_conventional-commits-filter@5.0.0/node_modules/conventional-changelog/node_modules:/home/narogozhina/work/study/2025-2026/2025-2--study--computer-practice/node_modules/.pnpm/conventional-changelog@7.1.1_conventional-commits-filter@5.0.0/node_modules:/home/narogozhina/work/study/2025-2026/2025-2--study--computer-practice/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/narogozhina/work/study/2025-2026/2025-2--study--computer-practice/node_modules/.pnpm/conventional-changelog@7.1.1_conventional-commits-filter@5.0.0/node_modules/conventional-changelog/dist/cli/node_modules:/home/narogozhina/work/study/2025-2026/2025-2--study--computer-practice/node_modules/.pnpm/conventional-changelog@7.1.1_conventional-commits-filter@5.0.0/node_modules/conventional-changelog/dist/node_modules:/home/narogozhina/work/study/2025-2026/2025-2--study--computer-practice/node_modules/.pnpm/conventional-changelog@7.1.1_conventional-commits-filter@5.0.0/node_modules/conventional-changelog/node_modules:/home/narogozhina/work/study/2025-2026/2025-2--study--computer-practice/node_modules/.pnpm/conventional-changelog@7.1.1_conventional-commits-filter@5.0.0/node_modules:/home/narogozhina/work/study/2025-2026/2025-2--study--computer-practice/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../conventional-changelog@7.1.1_conventional-commits-filter@5.0.0/node_modules/conventional-changelog/dist/cli/index.js" "$@"
else
  exec node  "$basedir/../../../../../conventional-changelog@7.1.1_conventional-commits-filter@5.0.0/node_modules/conventional-changelog/dist/cli/index.js" "$@"
fi
